    "@types/swagger-ui": "^5.21.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "serverless-offline": "^14.4.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.152",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "cors": "^2.8.5",
