{
  "name": "datacountries",
  "version": "1.0.0",
  "description": "api payant qui liste les inforamtions  des touts les countries",
  "main": "src/index.ts",
  "type": "commonjs",
  "scripts": {
  "build": "tsc",
  "deploy": "npm run build && sam deploy --region us-east-1",
  "deploy:prod": "npm run build && sam deploy --config-env prod",
  "start:local": "NODE_ENV=development node dist/index.js",
  "build:layer": "cd layer/nodejs && npm install --production && cd ../..",
  "dev": "ts-node --watch src/index.ts", 
  "sam:validate": "sam validate --template-file template.yml",
  "sam:local": "npm run build && sam local start-api",
  "sam:invoke": "npm run build && sam local invoke"
  },
